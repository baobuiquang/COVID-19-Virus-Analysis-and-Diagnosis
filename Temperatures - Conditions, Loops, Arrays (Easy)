#include <stdlib.h>
#include <stdio.h>
#include <string.h>
#include <stdbool.h>

/**
 * Auto-generated code below aims at helping you parse
 * the standard input according to the problem statement.
 **/

int main()
{
    // the number of temperatures to analyse
    int n;
    scanf("%d", &n);
    int result = 5526;
    for (int i = 0; i < n; i++) {
        // a temperature expressed as an integer ranging from -273 to 5526
        int t;
        scanf("%d", &t);
        //|t|
        int abs_t = t*(t>=0) + (-t)*(t<0);
        int abs_result = result*(result>=0) + (-result)*(result<0);
        if (abs_t < abs_result)
        {
            result = t;
        }
        else if (abs_t == abs_result)
        {
            if ((result < 0) && (t > 0))
            {
                result = -result;
            }
        }
    }

    // Write an action using printf(). DON'T FORGET THE TRAILING \n
    // To debug: fprintf(stderr, "Debug messages...\n");
    
    if (n == 0)
    {
        printf("0\n");
    }
    else
    {
        printf("%d\n", result);
    }

    return 0;
}
